{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:sonarjs/recommended",
        "plugin:unicorn/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "import/extensions": ["error", { "js": "ignorePackages" }]
    }
}
