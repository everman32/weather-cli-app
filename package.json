{
  "name": "weather-cli-app",
  "version": "1.0.0",
  "description": "Console application providing city weather",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "weather": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "prit": "prettier --check src/**/*.js",
    "prit:fix": "prettier --write src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [
    "weather",
    "cli",
    "nodejs"
  ],
  "author": "Victor Borodyn",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "husky": "^8.0.1",
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^13.0.0"
  }
}
